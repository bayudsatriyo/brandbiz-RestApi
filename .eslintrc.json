{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "no-console": "off",
        "import/no-extraneous-dependencies": false
    }
}
